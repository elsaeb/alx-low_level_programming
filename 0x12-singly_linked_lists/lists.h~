#ifndef _LISTS_H_
#define _LISTS_H_
#include <stdlib.h>

/**
 * struct list_s - singly linked list
 * @str: string to be listed
 * @len: length of the string
 * @next: points to the next node
 */
typedef struct list_s
{
	char *str;
	unsigned int len;
	struct list_s *next;
} list_t;

size_t print_list(const list_t *head);

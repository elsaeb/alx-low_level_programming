#include "main.h"

/**
 * print_rev - prints string in reverse order
 * @s: input string
 * Return: nothing
 */
int _strlen(char *s);
void print_rev(char *s)
{
	int i = _strlen(s);

	while (s[i] != '\0')
	{
		_putchar(s[i]);
		i--;
	}
	_putchar('\n');
}
/**
 * _strlen - give the length of string
 * @s: input character pointer
 * Return: the length of the string
 */
int _strlen(char *s)
{
	int length = 0;

	while (s[length] != '\0')
		length++;
	return (length);
}


#include "main.h"

/**
 * print_to_98 - check the code
 * @n: an input character
 * prints number to 98
 * Return: nothing
 */
void print_to_98(int n)
{
	if (n <= 98)
	{
		int i;

		for (i=n; i <= 98; i++)
		{
			_putchar(i + '0');
			_putchar(',');
			_putchar(' ');
		}
	}
		else
		{
			if (n > 98)
			{
				int i;

				for (i = n; i <= 98; i--)
				{
					_putchar(i + '0');
					_putchar(',');
					_putchar(' ');
				}
			}
		}
}
					
			       

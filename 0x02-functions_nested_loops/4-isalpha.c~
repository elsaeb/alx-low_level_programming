#include "main.h"

/**
 * _isalpha -check the code
 * @c: an input character
 * Description: function uses _putchar to chech if a character is letter.
 * Return: 1 if c is lower or uppercase letter and 0 if c is not letter.
 */
int _isalpha(int c)
{
	char i;
	int letter = 0;

	for (i = 'a'; i <= 'z'; i++)
	{
		for (i = 'A'; i <= 'Z'; i++)
			if (c == i)
				letter = 1;
	}

	return (letter);
}
				
